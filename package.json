{
	"name": "hypeInbox",
	"version": "3.0.0",
	"description": "Enhanced messaging for Elgg",
	"homepage": "http://hypejunction.com/",
	"keywords": ["elgg", "plugin", "messages", "inbox", "threads"],
	"author": {
		"name": "hypejunction",
		"email": "ismayil@hypejunction.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hypeJunction/hypeInbox"
	},
	"license": "GPL-2.0",
	"copyright": "2011-2014 (c) Ismayil Khayredinov",
	"devDependencies": {
		"grunt": "~0.4.1",
		"grunt-composer": "~0.4.4",
		"grunt-contrib-compass": "~1.0.1",
		"grunt-contrib-watch": "~0.4.4",
		"grunt-version": "~0.3.0",
		"grunt-contrib-copy": "~0.7.0",
		"grunt-contrib-clean": "~0.6.0",
		"grunt-contrib-compress": "~0.12.0",
		"grunt-bower-task": "~0.4.0",
		"grunt-phpunit": "~0.3.6",
		"js2xmlparser": "~0.1.7"
	},
	"config": {
		"framework": {
			"dir": "inbox"
		},
		"plugin_manifest": {
			"category": ["messages"],
			"requires": [
				{
					"type": "elgg_release",
					"version": "1.9"
				},
				{
					"type": "plugin",
					"name": "elgg_tokeninput"
				}
			],
			"suggests" : [
				{
					"type": "plugin",
					"name": "hypeLists"
				},
				{
					"type": "plugin",
					"name": "hypeScraper"
				},
				{
					"type": "plugin",
					"name": "elgg_dropzone"
				}
			]
		}
	}
}